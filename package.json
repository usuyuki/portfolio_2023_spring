{
  "name": "portfolio-2023-spring",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "1": "pnpm format && (pnpm lint & pnpm check) && exit 0",
    "tests": "pnpm test:u & pnpm test:i",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "test:u": "vitest run --config vitest.config.ts",
    "test:i": "playwright test -c playwright.config.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.3",
    "@playwright/test": "^1.54.2",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-cloudflare": "^7.1.2",
    "@sveltejs/kit": "^2.27.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.0",
    "@testing-library/svelte": "^5.2.8",
    "@types/gtag.js": "^0.0.20",
    "@types/tryghost__content-api": "^1.3.17",
    "autoprefixer": "^10.4.21",
    "jsdom": "^25.0.1",
    "postcss": "^8.5.6",
    "svelte": "^5.37.2",
    "svelte-check": "^4.3.0",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "vite": "^7.0.6",
    "vitest": "^2.1.9",
    "wrangler": "^4.27.0"
  },
  "type": "module",
  "dependencies": {
    "@notionhq/client": "^4.0.1",
    "@tryghost/content-api": "^1.12.0",
    "embla-carousel": "^8.6.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-svelte": "^8.6.0"
  },
  "packageManager": "pnpm@8.14.1+sha1.d039b38e0b20ad012ed548e44267b8d4c88b447a"
}
