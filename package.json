{
	"name": "portfolio-2023-spring",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"1": "pnpm format && (pnpm lint & pnpm check) && exit 0",
		"tests": "pnpm test:u & pnpm test:i",
		"dev": "vite dev",
		"dev-cf": "wrangler dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "biome lint .",
		"format": "biome format --write .",
		"test:u": "vitest run --config vitest.config.ts",
		"test:i": "playwright test -c playwright.config.ts"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.2",
		"@playwright/test": "^1.56.1",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.48.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/postcss": "^4.1.16",
		"@testing-library/svelte": "^5.2.8",
		"@types/gtag.js": "^0.0.20",
		"@types/tryghost__content-api": "^1.3.17",
		"autoprefixer": "^10.4.21",
		"jsdom": "^27.1.0",
		"postcss": "^8.5.6",
		"svelte": "^5.43.2",
		"svelte-check": "^4.3.3",
		"tailwindcss": "^4.1.16",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3",
		"vite": "^7.1.12",
		"vitest": "^4.0.6",
		"wrangler": "^4.45.3"
	},
	"type": "module",
	"dependencies": {
		"@notionhq/client": "^5.3.0",
		"@tryghost/content-api": "^1.12.0",
		"embla-carousel": "^8.6.0",
		"embla-carousel-autoplay": "^8.6.0",
		"embla-carousel-svelte": "^8.6.0"
	},
	"packageManager": "pnpm@10.14.0",
	"engines": {
		"node": ">=22.0.0"
	}
}
