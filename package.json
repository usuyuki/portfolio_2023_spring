{
	"name": "portfolio-2023-spring",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"1": "pnpm format && (pnpm lint & pnpm check) && exit 0",
		"tests": "pnpm test:u & pnpm test:c & pnpm test:i",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:u": "vitest run",
		"test:c": "playwright test -c playwright-ct.config.ts",
		"test:i": "playwright test -c playwright.config.ts"
	},
	"devDependencies": {
		"@playwright/experimental-ct-svelte": "^1.38.1",
		"@playwright/test": "^1.38.1",
		"@sveltejs/adapter-auto": "^1.0.3",
		"@sveltejs/adapter-cloudflare-workers": "^1.1.4",
		"@sveltejs/kit": "^1.25.1",
		"@testing-library/svelte": "^3.2.2",
		"@types/gtag.js": "^0.0.12",
		"@types/tryghost__content-api": "^1.3.12",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.50.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"jsdom": "^21.1.2",
		"postcss": "^8.4.31",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^3.59.2",
		"svelte-check": "^3.5.2",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"typescript": "^4.9.5",
		"vite": "^4.4.9",
		"vitest": "^0.25.8",
		"wrangler": "^2.20.1"
	},
	"type": "module",
	"dependencies": {
		"@notionhq/client": "^2.2.13",
		"@tryghost/content-api": "^1.11.17",
		"embla-carousel": "^7.1.0",
		"embla-carousel-autoplay": "^7.1.0",
		"embla-carousel-svelte": "^7.1.0"
	}
}
