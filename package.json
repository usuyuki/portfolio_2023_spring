{
	"name": "portfolio-2023-spring",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"1": "pnpm format && (pnpm lint & pnpm check) && exit 0",
		"tests": "pnpm test:u & pnpm test:c & pnpm test:i",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:u": "vitest run",
		"test:c": "playwright test -c playwright-ct.config.ts",
		"test:i": "playwright test -c playwright.config.ts"
	},
	"devDependencies": {
		"@playwright/experimental-ct-svelte": "^1.49.1",
		"@playwright/test": "^1.49.1",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/adapter-cloudflare-workers": "^2.7.0",
		"@sveltejs/kit": "^2.15.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@testing-library/svelte": "^5.2.6",
		"@types/gtag.js": "^0.0.20",
		"@types/tryghost__content-api": "^1.3.17",
		"@typescript-eslint/eslint-plugin": "^8.19.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.17.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"jsdom": "^25.0.1",
		"postcss": "^8.4.49",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.2",
		"svelte": "^5.16.0",
		"svelte-check": "^4.1.1",
		"tailwindcss": "^3.4.17",
		"tslib": "^2.8.1",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.19.0",
		"vite": "^6.0.6",
		"vitest": "^2.1.9",
		"wrangler": "^3.99.0"
	},
	"type": "module",
	"dependencies": {
		"@notionhq/client": "^2.2.15",
		"@tryghost/content-api": "^1.11.21",
		"embla-carousel": "^8.5.1",
		"embla-carousel-autoplay": "^8.5.1",
		"embla-carousel-svelte": "^8.5.1"
	},
	"packageManager": "pnpm@8.14.1+sha1.d039b38e0b20ad012ed548e44267b8d4c88b447a"
}
