{
	"name": "portfolio-2023-spring",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"1": "pnpm format && (pnpm lint & pnpm check) && exit 0",
		"tests": "pnpm test:u & pnpm test:i",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:u": "vitest run --config vitest.config.ts",
		"test:i": "playwright test -c playwright.config.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.54.1",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/adapter-cloudflare": "^7.1.1",
		"@sveltejs/kit": "^2.26.1",
		"@sveltejs/vite-plugin-svelte": "^6.1.0",
		"@testing-library/svelte": "^5.2.8",
		"@types/gtag.js": "^0.0.20",
		"@types/tryghost__content-api": "^1.3.17",
		"@typescript-eslint/eslint-plugin": "^8.38.0",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^9.1.2",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^16.3.0",
		"jsdom": "^25.0.1",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.37.0",
		"svelte-check": "^4.3.0",
		"tailwindcss": "^3.4.17",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.38.0",
		"vite": "^7.0.6",
		"vitest": "^2.1.9",
		"wrangler": "^4.26.0"
	},
	"type": "module",
	"dependencies": {
		"@notionhq/client": "^4.0.1",
		"@tryghost/content-api": "^1.12.0",
		"embla-carousel": "^8.6.0",
		"embla-carousel-autoplay": "^8.6.0",
		"embla-carousel-svelte": "^8.6.0"
	},
	"packageManager": "pnpm@8.14.1+sha1.d039b38e0b20ad012ed548e44267b8d4c88b447a"
}
